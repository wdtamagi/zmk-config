	RC(0,0)     RC(2,0)  RC(0,1)     RC(2,1)  RC(3,2)     RC(1,2)  RC(3,3)     RC(1,3)     RC(3,4)     RC(1,4)  RC(3,5)     RC(1,5)  RC(0,6)     RC(2,6)   RC(0,7)     RC(2,7)
